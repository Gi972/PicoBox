pico-8 cartridge // http://www.pico-8.com
version 4
__lua__
x = 20
y = 20

gravity = 0.5

vx = 5
vy = 0

friction = 0.999

max_points = 100

bounce = 0.9


function make_point(x,y)
	local p = {}
	p.x = x
	p.y = y
	p.oldx = rnd(5) + x-1
	p.oldy = rnd(5) + y-1
	p.vx = vx
	p.vy = vy
	p.color = flr(6+rnd(6))
	
 if (count(points) < max_points) then
  add(points, p)
 end
 
	return p
end

function draw_point(p)
	 color(p.color)
  rectfill(p.x-1,p.y-1,p.x,p.y)
  
end

function move_point(p)
  p.vx = (p.x - p.oldx) * friction
  p.vy = (p.y - p.oldy) * friction
  
  p.oldx = p.x
  p.oldy = p.y
  
  p.x += p.vx
  p.y += p.vy
		
		p.y += gravity

		if(p.x > 128) then
		 p.x = 128
		 p.oldx = p.x + p.vx
		end
		
		if(p.x<0) then
		 p.x = 0
		 p.oldx = p.x + p.vx * bounce
		end
		
		if(p.y > 128) then 
   p.y = 128
   p.oldy = p.y + p.vy * bounce
		end
		
		if(p.y < 0) then
			p.y = 0
			p.oldy = p.y + p.vy * bounce
		end
		
end

function _update()

	 foreach(points, move_point)		
			
end

function _draw()
  color(0)
  rectfill(0,0,128,128)

  foreach(points, draw_point)
end


-- called at start by pico-8
function _init()
 points = {}
 
 -- spawn points
 for i=0,10000 do 
 	 pnt = make_point(i,rnd(10)+10)
 end
end


